{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/fonts/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/fonts/$1"
    },
    {
      "src": "/css/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/js/$1"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
